<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="474.0" prefWidth="369.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.laboratoryworks.Ciphers.CiphersController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Шифр с фиксированным ключом">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
        <TextField fx:id="startField1" layoutX="14.0" layoutY="43.0" prefHeight="33.0" prefWidth="345.0" promptText="Начальный текст" />
        <Button fx:id="buttonOne" layoutX="14.0" layoutY="83.0" mnemonicParsing="false" onAction="#codeWithoutKey" prefHeight="33.0" prefWidth="345.0" text="Зашифровать" />
        <Button fx:id="decodeButtonOne1" layoutX="14.0" layoutY="123.0" mnemonicParsing="false" onAction="#decodeWithoutKey" prefHeight="33.0" prefWidth="345.0" text="Дешифровать" />
        <TextField fx:id="resultField1" layoutX="14.0" layoutY="167.0" prefHeight="33.0" prefWidth="345.0" promptText="Результат" />
        <Label layoutX="14.0" layoutY="233.0" text="Шифр Цезаря с вводимым ключом">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
        <TextField fx:id="startField2" layoutX="12.0" layoutY="262.0" prefHeight="33.0" prefWidth="345.0" promptText="Начальный текст" />
        <TextField fx:id="keyField" layoutX="12.0" layoutY="301.0" prefHeight="33.0" prefWidth="345.0" promptText="Ключ" />
        <Button fx:id="buttonTwo" layoutX="12.0" layoutY="341.0" mnemonicParsing="false" onAction="#codeWithKey" prefHeight="33.0" prefWidth="345.0" text="Зашифровать с ключом" />
        <Button fx:id="decodeButtonTwo" layoutX="12.0" layoutY="384.0" mnemonicParsing="false" onAction="#decodeWithKey" prefHeight="33.0" prefWidth="345.0" text="Дешифровать с ключом" />
        <TextField fx:id="resultField2" layoutX="12.0" layoutY="425.0" prefHeight="33.0" prefWidth="345.0" promptText="Результат" />
    </children>
</Pane>
